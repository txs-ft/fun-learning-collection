          "defs": [
            {
              "level": "",
              "longDef": {
                "en": "an occasion at the end of a television programme, etc. when something that has been hidden or kept secret until then is shown to the audience",
                "cn": "（电视节目最后的）揭示"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "The final reveal of the new kitchen was a huge success.",
                  "cn": "最后大揭示的新厨房非常成功。"
                }
              ]
            }
          ]
        }
      ],
