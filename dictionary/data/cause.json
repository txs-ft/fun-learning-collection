          "defs": [
            {
              "level": "",
              "longDef": {
                "en": "because",
                "cn": "因为，由于"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "I'll host the party cause I've got plenty of room at my house.",
                  "cn": "因为我家地方大，所以将由我来举办聚会。"
                },
                {
                  "gram": "",
                  "en": "I try to practise my French every day, cause I'm not very good at it.",
                  "cn": "我的法语不太好，所以我尽可能地天天练习。"
                }
              ]
            }
          ]
