      "defGroups": [
        {
          "simple": "HEAD COVERING",
          "defs": [
            {
              "level": "",
              "longDef": {
                "en": "a circular decoration for the head, usually made of gold and jewels (= precious stones), and worn by a king or queen at official ceremonies",
                "cn": "王冠，皇冠;冕"
              },
              "examples": []
            },
            {
              "level": "",
              "longDef": {
                "en": "the act of winning a sports competition",
                "cn": "冠军"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "He plans to defend his Olympic crown.",
                  "cn": "他计划卫冕奥运会冠军。"
                }
              ]
            }
          ]
        },
        {
          "simple": "TOP PART",
          "defs": [
            {
              "level": "",
              "longDef": {
                "en": "the top part of a head, hat, or hill",
                "cn": "（头、帽或山的）顶部"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "A pink ribbon had been tied around the crown of the hat.",
                  "cn": "帽子顶部系着一条粉色的缎带。"
                }
              ]
            }
          ]
        },
        {
          "simple": "TOOTH",
          "defs": [
            {
              "level": "",
              "longDef": {
                "en": "an artificial piece used to cover a damaged tooth",
                "cn": "（受损牙齿的）人造冠"
              },
              "examples": []
            }
          ]
        },
        {
          "simple": "COIN",
          "defs": [
            {
              "level": "",
              "longDef": {
                "en": "a British coin that is no longer used",
                "cn": "克朗（英国旧币）"
              },
              "examples": []
            }
          ]
        }
      ],
      "syllableCount": 1,
      "stressIndex": 0
    },
