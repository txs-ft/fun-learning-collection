      "defGroups": [
        {
          "simple": "SEASON",
          "defs": [
            {
              "level": "A2",
              "longDef": {
                "en": "the season of the year between winter and summer, lasting from March to June north of the equator, and from September to December south of the equator, when the weather becomes warmer, leaves and plants start to grow again and flowers appear",
                "cn": "春天，春季"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "Many bulbs bloom in (the) spring.",
                  "cn": "很多球茎植物春天开花。"
                },
                {
                  "gram": "",
                  "en": "Janet's coming over for a couple of weeks next spring.",
                  "cn": "珍妮特明年春天要来这儿呆几个星期。"
                },
                {
                  "gram": "",
                  "en": "It's been a very wet spring.",
                  "cn": "今年春天雨很多。"
                },
                {
                  "gram": "",
                  "en": "a spring day/morning",
                  "cn": "春日白天／早晨"
                },
                {
                  "gram": "",
                  "en": "spring flowers/weather",
                  "cn": "春花／春日天气"
                }
              ]
            }
          ]
        },
        {
          "simple": "CURVED METAL",
          "defs": [
            {
              "level": "",
              "longDef": {
                "en": "a piece of curved or bent metal that can be pressed into a smaller space but then returns to its usual shape",
                "cn": "弹簧，发条"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "The children have jumped on the couch so much that they've ruined the springs.",
                  "cn": "孩子们老是在沙发上跳来跳去的，把弹簧都弄坏了。"
                }
              ]
            },
            {
              "level": "",
              "longDef": {
                "en": "something's ability to return to its usual shape after it has been pressed",
                "cn": "弹性，弹力"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "Over the years the mattress has lost its spring.",
                  "cn": "几年下来，床垫失去了弹性。"
                }
              ]
            }
          ]
        },
        {
          "simple": "WATER",
          "defs": [
            {
              "level": "C1",
              "longDef": {
                "en": "a place where water naturally flows out from the ground",
                "cn": "泉"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "bubbling/hot springs",
                  "cn": "汩汩冒泡的泉／温泉"
                }
              ]
            }
          ]
        }
      ],
