          "defs": [
            {
              "level": "A2",
              "longDef": {
                "en": "the round object that moves in the sky around the earth and can be seen at night",
                "cn": "月球，月亮"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "What time does the moon rise/set (= appear/disappear in the sky)?",
                  "cn": "月亮何时升起／落下？"
                }
              ]
            },
            {
              "level": "",
              "longDef": {
                "en": "the shape made by the amount of the moon that you can see at a particular time",
                "cn": "月相"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "There's no moon (= you cannot see the moon) tonight.",
                  "cn": "今晚没有月亮。"
                },
                {
                  "gram": "",
                  "en": "a crescent/full/new moon",
                  "cn": "弦／满／新月"
                }
              ]
            },
            {
              "level": "",
              "longDef": {
                "en": "a similar round object that moves around another planet",
                "cn": "卫星"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "Jupiter has at least 16 moons.",
                  "cn": "木星至少有16颗卫星。"
                }
              ]
            }
          ]
        }
