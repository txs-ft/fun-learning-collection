      "defGroups": [
        {
          "simple": "",
          "defs": [
            {
              "level": "C2",
              "longDef": {
                "en": "to take great care of something because you love it or consider it very valuable",
                "cn": "珍爱;珍惜;珍藏"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "I will always treasure those memories of my dad.",
                  "cn": "我会一直珍藏对爸爸的回忆。"
                },
                {
                  "gram": "",
                  "en": "This pen that my grandfather gave me is one of my most treasured possessions.",
                  "cn": "我祖父送给我的这支钢笔是我最珍爱的东西之一。"
                }
              ]
            }
          ]
        }
