      "defGroups": [
        {
          "simple": "JOURNEY",
          "defs": [
            {
              "level": "B1",
              "longDef": {
                "en": "an organized journey for a particular purpose",
                "cn": "远征;探险，考察"
              },
              "examples": [
                {
                  "en": "We're going on a shopping expedition on Saturday.",
                  "cn": "周六我们要去购物。"
                },
                {
                  "en": "Scott died while he was on an expedition to the Antarctic in 1912.",
                  "cn": "斯科特1912年死于南极探险途中。"
                }
              ]
            },
            {
              "level": null,
              "longDef": {
                "en": "the people, vehicles, animals, etc. taking part in an expedition",
                "cn": "踏上征途的一批人（或车、动物等）;考察队;探险队"
              },
              "examples": [
                {
                  "en": "The British expedition to Mount Everest is leaving next month.",
                  "cn": "英国珠穆朗玛峰探险队将于下月启程。"
                }
              ]
            }
          ]
        },
        {
          "simple": "SPEED",
          "defs": [
            {
              "level": null,
              "longDef": {
                "en": "speed in doing something",
                "cn": "迅速;快速;动作敏捷"
              },
              "examples": [
                {
                  "en": "We will deal with your order with the greatest possible expedition.",
                  "cn": "我们将尽快处理您的订单。"
                }
              ]
            }
          ]
        }
