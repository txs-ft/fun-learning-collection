      "defGroups": [
        {
          "simple": "",
          "defs": [
            {
              "level": "B1",
              "longDef": {
                "en": "an official document that states that the information on it is true",
                "cn": "证书;证明"
              },
              "examples": [
                {
                  "en": "a birth/marriage/death certificate",
                  "cn": "出生／结婚／死亡证明"
                },
                {
                  "en": "a doctor's/medical certificate",
                  "cn": "医生／医疗证明"
                }
              ]
            },
            {
              "level": "B1",
              "longDef": {
                "en": "the qualification that you receive when you are successful in an exam",
                "cn": "成绩合格证书;毕业证书"
              },
              "examples": [
                {
                  "en": "She has a Certificate in Drama Education.",
                  "cn": "她有戏剧教育资格证书。"
                }
              ]
            }
          ]
        }
      ],
      "syllableCount": 4,
      "stressIndex": 1
    },
