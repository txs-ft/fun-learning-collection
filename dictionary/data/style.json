      },
      "defGroups": [
        {
          "simple": "DESIGN",
          "defs": [
            {
              "level": null,
              "longDef": {
                "en": "to shape or design something such as a person's hair or an object like a piece of clothing or furniture or a book, especially so that it looks attractive",
                "cn": "设计;给…造型;（尤指）把…设计得吸引人"
              },
              "examples": [
                {
                  "en": "You've had your hair styled - it really suits you.",
                  "cn": "你做了头发——这个发型真适合你。"
                },
                {
                  "en": "This range of jackets is styled to look good whatever the occasion.",
                  "cn": "这个系列的夹克衫设计巧妙，适合各种场合穿着。"
                }
              ]
            }
          ]
        },
        {
          "simple": "TITLE",
          "defs": [
            {
              "level": null,
              "longDef": {
                "en": "to give a title to a person or group",
                "cn": "称呼;给…命名"
              },
              "examples": [
                {
                  "en": "She styles herself \"Doctor\" but she doesn't have a degree.",
                  "cn": "她自称“博士”，但实际上她根本没有学位。"
                }
              ]
            }
          ]
        }
      ],
      "syllableCount": 1,
