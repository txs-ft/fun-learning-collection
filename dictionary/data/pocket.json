          "defs": [
            {
              "level": "",
              "longDef": {
                "en": "used to describe something that is small enough to put in your pocket, or that you regularly carry in your pocket",
                "cn": "袖珍的，口袋版的"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "a pocket dictionary",
                  "cn": "袖珍词典"
                },
                {
                  "gram": "",
                  "en": "a pocket diary",
                  "cn": "袖珍日记本"
                },
                {
                  "gram": "",
                  "en": "a pocket calculator",
                  "cn": "袖珍计算器"
                }
              ]
            },
            {
              "level": "",
              "longDef": {
                "en": "smaller than usual",
                "cn": "很小的，小型的"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "a pocket battleship",
                  "cn": "小型战舰"
                }
              ]
            }
          ]
        }
      ],
