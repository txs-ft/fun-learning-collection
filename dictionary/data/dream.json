          "simple": "SLEEP",
          "defs": [
            {
              "level": "A2",
              "longDef": {
                "en": "a series of events or images that happen in your mind when you are sleeping",
                "cn": "梦，睡梦，梦境"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "a good/bad dream",
                  "cn": "好／噩梦"
                },
                {
                  "gram": "",
                  "en": "a recurring dream",
                  "cn": "反复出现的梦境"
                },
                {
                  "gram": "",
                  "en": "I had a very strange dream about you last night.",
                  "cn": "昨晚我梦见了你，那个梦非常奇怪。"
                },
                {
                  "gram": "[ + that ]",
                  "en": "Paul had a dream that he won the lottery.",
                  "cn": "保罗梦见自己彩票中了奖。"
                }
              ]
            }
          ]
        },
        {
          "simple": "HOPE",
          "defs": [
            {
              "level": "B1",
              "longDef": {
                "en": "something that you want to happen very much but that is not very likely",
                "cn": "梦想，幻想，愿望"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "It's always been my dream to have flying lessons.",
                  "cn": "学习飞行一直是我的梦想。"
                },
                {
                  "gram": "",
                  "en": "Winning all that money was a dream come true.",
                  "cn": "赢得了那些钱，终于美梦成真了。"
                }
              ]
            },
            {
              "level": "",
              "longDef": {
                "en": "the best that you can imagine:",
                "cn": "参加我们超级棒的竞赛，赢得您梦想中的房子！"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "Win the house of your dreams in our fantastic competition!",
                  "cn": "参加我们超级棒的竞赛，赢得您梦想中的房子！"
                }
              ]
            }
          ]
        }
