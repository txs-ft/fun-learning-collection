      "defGroups": [
        {
          "simple": "",
          "defs": [
            {
              "level": null,
              "longDef": {
                "en": "the area or border around something",
                "cn": "周围地带;围绕物;缘饰"
              },
              "examples": [
                {
                  "en": "Our bathtub has a tiled surround.",
                  "cn": "我们的浴缸周围铺了瓷砖。"
                }
              ]
            },
            {
              "level": null,
              "longDef": {
                "en": "the area that is near and around a place",
                "cn": "周边，附近"
              },
              "examples": [
                {
                  "en": "The house is set in lush surrounds and has views of the ocean.",
                  "cn": "小楼绿树环绕，可以看见大海。"
                }
              ]
            }
          ]
        }
      ],
      "syllableCount": 2,
      "stressIndex": 1
    }
