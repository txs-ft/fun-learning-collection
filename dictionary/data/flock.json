          "simple": "GROUP",
          "defs": [
            {
              "level": "",
              "longDef": {
                "en": "a group of sheep, goats, or birds",
                "cn": "羊群;鸟群;人群"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "a flock of sheep/goats/geese",
                  "cn": "一群绵羊／山羊／鹅"
                },
                {
                  "gram": "",
                  "en": "The shepherd is bringing his flock down from the hills.",
                  "cn": "牧羊人正把羊群往山下赶。"
                }
              ]
            },
            {
              "level": "",
              "longDef": {
                "en": "a large group of people, usually people of a particular type",
                "cn": "（特定类型的）一群人"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "A noisy flock of tourists came into the building.",
                  "cn": "一群吵吵嚷嚷的游客走进了大楼。"
                },
                {
                  "gram": "",
                  "en": "The priest invited all the members of his flock (= all the people who go to his church) to attend the special service.",
                  "cn": "牧师邀请教区的所有信徒参加这一特别仪式。"
                }
              ]
            }
          ]
        },
        {
          "simple": "MATERIAL",
          "defs": [
            {
              "level": "",
              "longDef": {
                "en": "soft material used for filling objects such as cushions, or soft material that forms a raised pattern on wallpaper or curtains",
                "cn": "（充填垫子等的）软填料；（在墙纸或窗帘上制作凸起图案用的）短绒，绒屑"
              },
              "examples": []
            }
          ]
        }
