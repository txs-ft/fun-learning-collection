          ]
        },
        {
          "simple": "MARK",
          "defs": [
            {
              "level": "",
              "longDef": {
                "en": "a mark in an exam or for a piece of work that shows that your work is considered excellent",
                "cn": "（学业成绩）A，优，甲"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "Sophie got an A for English.",
                  "cn": "苏菲英语得了个 A。"
                },
                {
                  "gram": "",
                  "en": "She got straight As (= all her marks were As) in her final exams.",
                  "cn": "她期末考试各科成绩均为 A。"
                },
                {
                  "gram": "",
                  "en": "Jim is a straight-A student (= all his marks are A).",
                  "cn": "吉姆是个全优生。"
                }
              ]
            }
          ]
        }
