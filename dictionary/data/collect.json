            {
              "level": "",
              "longDef": {
                "en": "When you call collect or make a collect phone call, the person you call pays for the call.",
                "cn": "对方付话费的"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "I'd like to make a collect call.",
                  "cn": "我想打一个对方付费的电话。"
                },
                {
                  "gram": "",
                  "en": "She called me collect.",
                  "cn": "她给我打了电话，由我付话费。"
                }
              ]
            }
          ]
