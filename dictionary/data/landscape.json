          "simple": "LAND",
          "defs": [
            {
              "level": "B1",
              "longDef": {
                "en": "a large area of land, especially in relation to its appearance",
                "cn": "乡间，野外;（尤指乡村的）风景，景色"
              },
              "examples": [
                {
                  "en": "a rural/barren landscape",
                  "cn": "乡村／荒原"
                },
                {
                  "en": "Demolition firms and builders are busy changing urban landscapes.",
                  "cn": ""
                },
                {
                  "en": "The landscape is dotted with the tents of campers and hikers.",
                  "cn": "乡间散布着宿营者和远足者搭起的帐篷。"
                },
                {
                  "en": "The cathedral dominates the landscape for miles around.",
                  "cn": "大教堂在方圆数英里的乡间高高耸立着。"
                }
              ]
            },
            {
              "level": null,
              "longDef": {
                "en": "a view or picture of the countryside, or the art of making such pictures",
                "cn": "乡村风景;乡村风景画;乡村风景绘画"
              },
              "examples": [
                {
                  "en": "a watercolour landscape",
                  "cn": "水彩风景画"
                },
                {
                  "en": "J.M.W. Turner is one of the best-known British landscape painters.",
                  "cn": "J.M.W.特纳是英国最著名的风景画画家之一。"
                }
              ]
            }
          ]
        }
