          "defs": [
            {
              "level": "C1",
              "longDef": {
                "en": "an extremely strong dislike",
                "cn": "憎恨，憎恶"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "She gave him a look of pure hate.",
                  "cn": "她非常憎恨地看了他一眼。"
                },
                {
                  "gram": "",
                  "en": "The feelings of hate grew stronger every day.",
                  "cn": "仇恨一天天加深。"
                },
                {
                  "gram": "",
                  "en": "One of my pet hates (= one of the main things I dislike) is people who use your name all the while when they're speaking to you.",
                  "cn": "我最讨厌那些跟你说话时不停地叫你名字的人。"
                }
              ]
            }
          ]
        }
