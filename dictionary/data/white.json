          "simple": "COLOUR",
          "defs": [
            {
              "level": "A1",
              "longDef": {
                "en": "of a colour like that of snow, milk, or bone",
                "cn": "白色的;雪白的;乳白的"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "a white T-shirt",
                  "cn": "白色的T恤衫"
                },
                {
                  "gram": "",
                  "en": "white walls",
                  "cn": "白墙"
                },
                {
                  "gram": "",
                  "en": "a black and white dog",
                  "cn": "黑白相间的狗"
                },
                {
                  "gram": "",
                  "en": "He's white-haired now.",
                  "cn": "他现在头发都已经白了。"
                },
                {
                  "gram": "",
                  "en": "\"How do you like your coffee?\" \"White (= with milk or cream) and no sugar, please.\"",
                  "cn": "“你想要什么样的咖啡？”“请加奶但不要加糖。”"
                }
              ]
            },
            {
              "level": "C2",
              "longDef": {
                "en": "having a pale face because you are not well, or you are feeling shocked",
                "cn": "（因为生病或受到惊吓）面色惨白的，苍白的"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "She was white, and her lips were pale.",
                  "cn": "她面色苍白，双唇没有血色。"
                }
              ]
            },
            {
              "level": "A2",
              "longDef": {
                "en": "used in the names of various food and drink products, many of which are not pure white but slightly cream, yellow, grey, or transparent",
                "cn": "（用于食品名称中）白…"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "white bread",
                  "cn": "白面包"
                },
                {
                  "gram": "",
                  "en": "white chocolate",
                  "cn": "白巧克力"
                },
                {
                  "gram": "",
                  "en": "white flour",
                  "cn": "白面"
                },
                {
                  "gram": "",
                  "en": "white sugar",
                  "cn": "白糖"
                },
                {
                  "gram": "",
                  "en": "white wine",
                  "cn": "白葡萄酒"
                }
              ]
            }
          ]
        },
        {
          "simple": "PEOPLE",
          "defs": [
            {
              "level": "B1",
              "longDef": {
                "en": "belonging to the group of people with skin that is pale in colour, who come from or whose family originally came from Europe",
                "cn": "白人（的）；白种人（的）"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "He had a black mother and a white father.",
                  "cn": "他的母亲是黑人，父亲是白人。"
                },
                {
                  "gram": "",
                  "en": "a predominantly white neighbourhood",
                  "cn": "以白人为主的社区"
                }
              ]
            }
          ]
        }
