          "defs": [
            {
              "level": "B1",
              "longDef": {
                "en": "a particular series of movements that you perform to music or the type of music that is connected with it",
                "cn": "舞蹈，舞步;舞曲"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "The band played a slow dance.",
                  "cn": "乐队演奏了一支舒缓的舞曲。"
                }
              ]
            },
            {
              "level": "A2",
              "longDef": {
                "en": "a social occasion where people dance, especially a formal occasion in a large room",
                "cn": "（尤指正式的）舞会"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "They're having an end-of-term dinner-dance.",
                  "cn": "他们要举办一场期末餐舞会。"
                }
              ]
            },
            {
              "level": "A1",
              "longDef": {
                "en": "the act of moving your feet and body to music",
                "cn": "舞，跳舞"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "We had a dance.",
                  "cn": "我们跳了一支舞。"
                },
                {
                  "gram": "",
                  "en": "The bride's father gets the first dance at the wedding.",
                  "cn": "新娘的父亲在婚礼上跟新娘跳第一支舞。"
                },
                {
                  "gram": "",
                  "en": "a dance class",
                  "cn": "舞蹈课"
                }
              ]
            },
            {
              "level": "A2",
              "longDef": {
                "en": "the art of performing dances, especially as a form of entertainment",
                "cn": "舞蹈表演;舞蹈艺术"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "The performers tell the story through song and dance.",
                  "cn": "演员们载歌载舞演绎了那个故事。"
