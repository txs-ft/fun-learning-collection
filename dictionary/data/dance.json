          "defs": [
            {
              "level": "A1",
              "longDef": {
                "en": "to move the body and feet to music",
                "cn": "跳（舞），舞蹈"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "We danced all night.",
                  "cn": "我们跳了一夜的舞。"
                },
                {
                  "gram": "",
                  "en": "We went dancing at a club.",
                  "cn": "我们去夜总会跳舞了。"
                },
                {
                  "gram": "",
                  "en": "What kind of music do you like dancing to?",
                  "cn": "你喜欢伴着哪种曲子跳舞？"
                },
                {
                  "gram": "",
                  "en": "Who was she dancing with at the party last night?",
                  "cn": "昨晚舞会上她在和谁跳舞？"
                },
                {
                  "gram": "",
                  "en": "Can you dance the tango?",
                  "cn": "你会跳探戈舞吗？"
                },
                {
                  "gram": "",
                  "en": "We played our music and danced the night away.",
                  "cn": "我们放上音乐，跳了一晚上的舞。"
                }
              ]
            },
            {
              "level": "",
              "longDef": {
                "en": "to move quickly and lightly",
                "cn": "（轻快地）舞动;摇曳;跳动"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "The daffodils were dancing in the breeze.",
                  "cn": "黄水仙在微风中轻轻舞动。"
                },
                {
                  "gram": "",
                  "en": "She watched the sunlight dancing on the water.",
                  "cn": "她注视着水面上的粼粼波光。"
                }
              ]
            }
          ]
        }
