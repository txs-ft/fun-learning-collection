      "defGroups": [
        {
          "simple": "SAY WORDS",
          "defs": [
            {
              "level": "A1",
              "longDef": {
                "en": "to say words aloud; to speak to someone",
                "cn": "讲话，谈话;谈论"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "We were just talking about Gareth's new girlfriend.",
                  "cn": "我们刚才在谈论加雷斯的新女友。"
                },
                {
                  "gram": "",
                  "en": "My little girl has just started to talk.",
                  "cn": "我的小女儿刚开始说话。"
                },
                {
                  "gram": "",
                  "en": "She talks to her mother on the phone every week.",
                  "cn": "她每周都跟母亲通电话。"
                }
              ]
            }
          ]
        },
        {
          "simple": "DISCUSS",
          "defs": [
            {
              "level": "B2",
              "longDef": {
                "en": "to discuss something with someone, often to try to find a solution to a disagreement",
                "cn": "（为了寻求解决分歧的途径而）与…会谈"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "The two sides have agreed to talk.",
                  "cn": "双方同意会谈。"
                }
              ]
            },
            {
              "level": "C1",
              "longDef": {
                "en": "to discuss a particular subject",
                "cn": "谈论正事／政治（等等）"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "Whenever they're together, they talk politics.",
                  "cn": "他们一到一起就谈论政治。"
                }
              ]
            }
          ]
        },
        {
          "simple": "LECTURE",
          "defs": [
            {
              "level": "B2",
              "longDef": {
                "en": "to give a lecture on a subject",
                "cn": "（就某一主题）作报告"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "The next speaker will be talking about endangered insects.",
                  "cn": "下一位讲者将就濒危昆虫作报告。"
                }
              ]
            }
          ]
        }
      ],
      "syllableCount": 1,
      "stressIndex": 0
    },
