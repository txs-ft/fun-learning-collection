          "defs": [
            {
              "level": "",
              "longDef": {
                "en": "to represent an area of land in the form of a map",
                "cn": "绘制…的地图;在地图上标示出"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "Parts of the mountainous region in the north of the country have still not been mapped.",
                  "cn": "人们还未为这个国家北部的部分山区绘制地图。"
                }
              ]
            }
          ]
        }
