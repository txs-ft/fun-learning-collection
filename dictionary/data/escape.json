      "defGroups": [
        {
          "simple": "GET FREE",
          "defs": [
            {
              "level": "B1",
              "longDef": {
                "en": "to get free from something, or to avoid something",
                "cn": "逃走，逃脱，逃离;逃避，避开"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "Two prisoners have escaped.",
                  "cn": "两个犯人逃走了。"
                },
                {
                  "gram": "",
                  "en": "A lion has escaped from its cage.",
                  "cn": "一头狮子逃出了兽笼。"
                },
                {
                  "gram": "",
                  "en": "She was lucky to escape serious injury.",
                  "cn": "她真是万幸，逃过一劫，没有受重伤。"
                },
                {
                  "gram": "",
                  "en": "He narrowly (= only just) escaped a fine.",
                  "cn": "他差一点被罚款。"
                },
                {
                  "gram": "",
                  "en": "His name escapes me (= I have forgotten his name).",
                  "cn": "我记不起他的名字了。"
                },
                {
                  "gram": "",
                  "en": "Nothing important escapes her notice/attention.",
                  "cn": "任何重要的事情都不会逃过她的眼睛／注意。"
                }
              ]
            }
          ]
        },
        {
          "simple": "COMPUTER",
          "defs": [
            {
              "level": "",
              "longDef": {
                "en": "to press the key on a computer keyboard that allows you to leave a particular screen and return to the previous one or to interrupt a process",
                "cn": "退出，结束（计算机运行程序）"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "Escape from this window and return to the main menu.",
                  "cn": "从这个窗口退出，返回主菜单。"
                }
              ]
            }
          ]
        }
      ],
      "syllableCount": 2,
      "stressIndex": 1
    },
