      "defGroups": [
        {
          "simple": "GET FREE",
          "defs": [
            {
              "level": "C1",
              "longDef": {
                "en": "the act of successfully getting out of a place or a dangerous or bad situation",
                "cn": "逃脱，逃离"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "He made his escape on the back of a motorcycle.",
                  "cn": "他骑上摩托车逃之夭夭。"
                },
                {
                  "gram": "",
                  "en": "an escape route",
                  "cn": "逃跑路线"
                },
                {
                  "gram": "",
                  "en": "They had a narrow escape (= only just avoided injury or death) when their car crashed.",
                  "cn": "撞车事故中，他们九死一生，逃过一劫。"
                }
              ]
            },
            {
              "level": "",
              "longDef": {
                "en": "a loss that happens by accident",
                "cn": "漏出；泄漏；溢出"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "an escape of radioactivity",
                  "cn": "放射线泄漏"
                }
              ]
            }
          ]
        },
        {
          "simple": "FORGET",
          "defs": [
            {
              "level": "B2",
              "longDef": {
                "en": "something that helps you to forget about your usual life or problems",
                "cn": "（对现实的）逃避，回避；解脱之物"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "Romantic novels provide an escape from reality.",
                  "cn": "浪漫小说提供了一种逃避现实的方式。"
                }
              ]
            }
          ]
        },
        {
          "simple": "COMPUTER",
          "defs": [
            {
              "level": "",
              "longDef": {
                "en": "the key on a computer keyboard that allows you to leave a particular screen and return to the previous one or to interrupt a process",
                "cn": "（计算机键盘上的）退出键"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "Press Esc to return to the main menu.",
                  "cn": "按退出键返回主菜单。"
                }
              ]
            }
          ]
        }
