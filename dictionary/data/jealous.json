          "defs": [
            {
              "level": "B1",
              "longDef": {
                "en": "upset and angry because someone that you love seems interested in another person",
                "cn": "妒忌的；好妒忌的；吃醋的"
              },
              "examples": [
                {
                  "gram": null,
                  "en": "a jealous husband/wife",
                  "cn": "吃醋的丈夫／妻子"
                },
                {
                  "gram": null,
                  "en": "Anna says she feels jealous every time another woman looks at her boyfriend.",
                  "cn": "安娜说每次别的女人看她的男朋友她都会感到妒忌。"
                }
              ]
            },
            {
              "level": "B2",
              "longDef": {
                "en": "unhappy and angry because someone has something that you want",
                "cn": "嫉妒的"
              },
              "examples": [
                {
                  "gram": null,
                  "en": "He had always been very jealous of his brother's good looks.",
                  "cn": "他兄弟的外貌总是让他嫉妒。"
                }
              ]
            }
          ]
        },
        {
          "simple": "CAREFUL",
