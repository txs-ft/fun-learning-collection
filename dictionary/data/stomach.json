          "simple": null,
          "defs": [
            {
              "level": null,
              "longDef": {
                "en": "to be able to accept an unpleasant idea or watch something unpleasant",
                "cn": "能够容忍，承受得住（令人不快的想法或景象）"
              },
              "examples": [
                {
                  "gram": null,
                  "en": "He can't stomach the idea that Peter might be the next chairman.",
                  "cn": "他一想到彼得可能会出任下届主席就很不舒服。"
                },
                {
                  "gram": null,
                  "en": "She found the violence in the film hard to stomach.",
                  "cn": "她觉得这部电影中的暴力场面令人难以忍受。"
                }
              ]
            }
          ]
        }
