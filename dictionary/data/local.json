      "defGroups": [
        {
          "simple": "PERSON",
          "defs": [
            {
              "level": "C2",
              "longDef": {
                "en": "a person who lives in the particular small area that you are talking about",
                "cn": "当地人，本地人"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "The café is popular with both locals and visitors.",
                  "cn": "这家咖啡馆很受当地人和游客的青睐。"
                }
              ]
            }
          ]
        },
        {
          "simple": "PUB",
          "defs": [
            {
              "level": "",
              "longDef": {
                "en": "a pub near to where a person lives, especially if they often go there to drink",
                "cn": "（尤指经常去的）家附近的酒馆"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "The George is my local.",
                  "cn": "乔治酒馆是我常去的邻近小酒馆。"
                }
              ]
            }
          ]
        },
        {
          "simple": "VEHICLE",
          "defs": [
            {
              "level": "",
              "longDef": {
                "en": "a train or bus that makes all or most of the stops along its route, allowing passengers to get on or off",
                "cn": "（指火车或汽车的）慢车"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "the 12.24 local to Poughkeepsie",
                  "cn": "12点24分到波基普西的慢车"
                }
              ]
            }
          ]
        },
        {
          "simple": "ORGANIZATION",
          "defs": [
            {
              "level": "",
              "longDef": {
                "en": "a division within a union representing people from a particular area",
                "cn": "（尤指全国性工会组织的）当地分支机构"
              },
              "examples": []
            }
          ]
        }
      ],
      "syllableCount": 2,
      "stressIndex": 0
    }
