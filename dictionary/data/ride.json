          "defs": [
            {
              "level": "B1",
              "longDef": {
                "en": "a journey on a horse or bicycle, or in a vehicle",
                "cn": "（骑马、骑车或乘坐车辆的）旅行，行程"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "It's a short bus ride to the airport.",
                  "cn": "坐公共汽车去机场很近。"
                },
                {
                  "gram": "",
                  "en": "I went for a (horse) ride last Saturday.",
                  "cn": "上周六我（骑马）去兜了一圈。"
                },
                {
                  "gram": "",
                  "en": "Do you want to come for a ride on my bike?",
                  "cn": "你想坐我的摩托车去兜兜风吗？"
                }
              ]
            },
            {
              "level": "B1",
              "longDef": {
                "en": "a free journey in a car to a place where you want to go",
                "cn": "乘便车;搭乘免费车"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "He asked me for a ride into town.",
                  "cn": "他请求搭我的便车进城。"
                }
              ]
            },
            {
              "level": "",
              "longDef": {
                "en": "a person who gives you a ride in their car",
                "cn": "让你搭便车的人"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "Well, I have to go - my ride is here.",
                  "cn": "好了，我得走了。让我搭车的人已经来了。"
                }
              ]
            },
            {
              "level": "",
              "longDef": {
                "en": "someone's car",
                "cn": "（某人的）车"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "Hey, nice ride.",
                  "cn": "哎，好车。"
                }
              ]
            },
            {
              "level": "B1",
              "longDef": {
                "en": "a machine in an amusement park that people travel in or are moved around by for entertainment",
                "cn": "（游乐园里）供人乘坐的游乐装置"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "We went on all the rides.",
                  "cn": "我们玩了游乐园里所有的装置。"
                },
                {
                  "gram": "",
                  "en": "My favourite ride is the Ferris wheel.",
                  "cn": "我最爱坐摩天轮。"
                }
              ]
            }
          ]
        }
