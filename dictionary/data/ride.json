          "defs": [
            {
              "level": "A1",
              "longDef": {
                "en": "to sit on something such as a bicycle, motorbike, or horse and travel along on it controlling its movements",
                "cn": "骑（自行车、摩托车或马等）"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "I learned to ride a bike when I was six.",
                  "cn": "我6岁学会了骑自行车。"
                },
                {
                  "gram": "",
                  "en": "I ride my moped to work.",
                  "cn": "我骑机动自行车去上班。"
                },
                {
                  "gram": "",
                  "en": "I ride to work on my moped.",
                  "cn": "我骑机动自行车去上班。"
                },
                {
                  "gram": "",
                  "en": "The hunters came riding by/past on their horses.",
                  "cn": "猎人们骑马飞驰而过。"
                }
              ]
            },
            {
              "level": "",
              "longDef": {
                "en": "to ride a horse",
                "cn": "骑马"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "Can you ride?",
                  "cn": "你会骑马吗？"
                },
                {
                  "gram": "",
                  "en": "Their daughter is learning to ride.",
                  "cn": "他们的女儿正在学骑马。"
                },
                {
                  "gram": "",
                  "en": "He rides well/badly.",
                  "cn": "他骑术很好／差。"
                }
              ]
            },
            {
              "level": "A2",
              "longDef": {
                "en": "to travel in a vehicle, such as a car, bus, or train",
                "cn": "乘（车），坐（车）；搭乘"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "He doesn't have a car so he rides to work on the bus.",
                  "cn": "他没有私家车，所以就坐公共汽车去上班。"
                },
                {
                  "gram": "",
                  "en": "We rode the train from San Diego to Portland.",
                  "cn": "我们从圣地亚哥坐火车到波特兰。"
                }
              ]
            },
            {
              "level": "",
              "longDef": {
                "en": "to try to control someone and force them to work",
                "cn": "驾驭，试图控制（某人）并强迫其工作"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "Your boss is riding you much too hard at the moment.",
                  "cn": "你的老板现在把你管得太严了。"
                }
              ]
            }
          ]
        }
