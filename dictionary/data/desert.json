          "defs": [
            {
              "level": "A2",
              "longDef": {
                "en": "an area, often covered with sand or rocks, where there is very little rain and not many plants",
                "cn": "沙漠，荒漠"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "They were lost in the desert for nine days.",
                  "cn": "他们在沙漠里迷路已经有9天了。"
                },
                {
                  "gram": "",
                  "en": "We had to cross a large area of arid, featureless desert.",
                  "cn": "我们必须穿越一大片干燥荒芜的沙漠。"
                },
                {
                  "gram": "",
                  "en": "the desert sun",
                  "cn": "沙漠里的太阳"
                }
              ]
            },
            {
              "level": "",
              "longDef": {
                "en": "a place that is considered to have no cultural, intellectual, etc. quality or interest:",
                "cn": "这个城镇是一片文化沙漠。"
              },
              "examples": [
                {
                  "gram": "",
                  "en": "This town is a cultural desert.",
                  "cn": "这个城镇是一片文化沙漠。"
                }
              ]
            }
          ]
